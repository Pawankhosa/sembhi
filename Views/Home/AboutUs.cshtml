                                                    <div class="sep"></div>
                                                    <h3 class="heading">COMPLETED PROJECTS</h3>
                                                </div>
                                            </div>
                                        </div><!-- /.wprt-counter -->
                                    </div><!-- /.wprt-animation-block -->

